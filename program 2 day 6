#include <stdio.h>

int main() 
{
    int n, sum = 0, total = 0;
    printf("enter the number of numbers");
    scanf("%d", &n);
    int nums[n];
    printf("enter the Array elements");
    for (int i = 0; i < n; i++) 
    {
        scanf("%d", &nums[i]);
        sum += nums[i];
    }
    total = n * (n + 1) / 2;
    printf("%d\n", total - sum);
    return 0;
}
